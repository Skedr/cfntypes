    "@typescript-eslint/parser": "^2.15.0",
    "debug": "^4.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "semver": "^7.3.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "awsResourceSpecificationVersion": "68.0.0"
