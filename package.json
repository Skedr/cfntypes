{
  "name": "@fmtk/cfntypes",
  "version": "0.2.1",
  "main": "lib/bundle.js",
  "module": "lib/bundle.esm.js",
  "types": "lib/index.d.ts",
  "author": "Gordon Leigh <gordon.leigh@futurematik.co.uk>",
  "repository": "https://github.com/futurematik/cfntypes",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "yarn clean && yarn generate && yarn compile",
    "bumpfeat": "npm version minor && npm publish",
    "bumpfix": "npm version patch && npm publish",
    "bumpmajor": "npm version major && npm publish",
    "clean": "rm -rf lib/ dist/",
    "compile": "tsc",
    "generate": "node -r ts-node/register src/generate.ts",
    "lint": "eslint src/ --ext=ts,tsx",
    "postversion": "git push --follow-tags",
    "prepare": "yarn build",
    "preversion": "yarn build",
    "watch:compile": "tsc -w --incremental"
  },
  "devDependencies": {
    "@fmtk/cfnspec": "^0.2.4",
    "@types/debug": "^4.1.5",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.9.2",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "debug": "^4.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "awsResourceSpecificationVersion": "11.6.0"
}